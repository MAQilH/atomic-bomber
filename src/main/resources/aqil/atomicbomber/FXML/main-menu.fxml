<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<AnchorPane prefHeight="512.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox fx:id="info_count" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                   <ImageView fx:id="avatar_img" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                       <clip>
                           <Circle centerX="75" centerY="75" radius="75" />
                       </clip>
                       <image>
                           <Image url="@../../../Images/avatar1.png" />
                       </image>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                   </ImageView>
                  <Label fx:id="username_lbl" styleClass="username-lbl" text="AQil" />
               </children>
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox alignment="CENTER" spacing="5.0" styleClass="start_game_count" VBox.vgrow="NEVER">
                     <children>
                        <Button fx:id="new_game_lbl" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="New Game" HBox.hgrow="ALWAYS" />
                        <VBox prefWidth="3.0" styleClass="line_count" />
                        <Button fx:id="saved_game_btn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="42.0" prefWidth="105.0" text="Saved Game" HBox.hgrow="ALWAYS" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" left="25.0" right="25.0" />
                     </VBox.margin></HBox>
                  <GridPane hgap="2.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" vgap="16.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Button fx:id="profile_btn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Profile" />
                        <Button fx:id="score_board_btn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Score Boadrd" GridPane.columnIndex="1" />
                        <Button fx:id="setting_btn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Setting" GridPane.rowIndex="1" />
                        <Button fx:id="exit_btn" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Exit" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                     <VBox.margin>
                        <Insets left="25.0" right="25.0" />
                     </VBox.margin>
                  </GridPane>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../Style/main-menu.css" />
      <URL value="@../Style/menu.css" />
   </stylesheets>
</AnchorPane>
