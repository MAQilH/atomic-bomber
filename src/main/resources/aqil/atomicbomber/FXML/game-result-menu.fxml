<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="menu_count" maxHeight="250.0" maxWidth="250.0" minHeight="250.0" minWidth="250.0" prefHeight="250.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aqil.atomicbomber.view.GameResultMenuController">
   <stylesheets>
      <URL value="@../Style/menu.css" />
      <URL value="@../Style/game-result-menu.css" />
   </stylesheets>
   <AnchorPane prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
         <Label fx:id="game_status_lbl" styleClass="win_lbl" text="WIN" />
         <VBox alignment="TOP_CENTER" prefWidth="300.0" spacing="10.0">
            <VBox.margin>
               <Insets top="15.0" />
            </VBox.margin>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="300.0">
               <Label text="Wave">
                  <HBox.margin>
                     <Insets right="5.0" />
                  </HBox.margin>
               </Label>
               <Label fx:id="wave_lbl" text="34">
                  <HBox.margin>
                     <Insets left="5.0" />
                  </HBox.margin>
               </Label>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="0.0" prefWidth="300.0">
               <Label text="Killing">
                  <HBox.margin>
                     <Insets right="5.0" />
                  </HBox.margin>
               </Label>
               <Label fx:id="killing_lbl" text="34">
                  <HBox.margin>
                     <Insets left="5.0" />
                  </HBox.margin>
               </Label>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="0.0" prefWidth="300.0">
               <Label text="Accuracy">
                  <HBox.margin>
                     <Insets right="5.0" />
                  </HBox.margin>
               </Label>
               <Label fx:id="accuracy_lbl" text="34">
                  <HBox.margin>
                     <Insets left="5.0" />
                  </HBox.margin>
               </Label>
            </HBox>
         </VBox>
      </VBox>
   </AnchorPane>
   <Button fx:id="save_btn" mnemonicParsing="false" text="Save/Back" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
</AnchorPane>
